    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/favoriteImageView"
            android:layout_width="80dp"
            android:layout_height="40dp"
            android:src="@drawable/ic_connect"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            app:tint="@android:color/holo_blue_light"/>

        <TextView
            android:id="@+id/textViewException"
            android:layout_width="270dp"
            android:layout_height="40dp"
            android:text=""
            android:textAlignment="center"
            android:textSize="14sp"
            android:layout_toRightOf="@+id/favoriteImageView"/>




    <TextView
        android:id="@+id/textViewItemInputNum"
        android:layout_width="80dp"
        android:layout_height="40dp"

        android:text="入庫No."
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/favoriteImageView"/>

    <EditText
        android:id="@+id/editTextItemInputNumber"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:layout_toRightOf="@+id/textViewItemInputNum"
        android:layout_below="@+id/favoriteImageView"
        android:background="@drawable/simple_black_flame"
        android:inputType="number"
        android:text="" />

    <TextView
        android:id="@+id/textViewAuthName"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="登録者"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemInputNum"
        />

    <EditText
        android:id="@+id/editTextAuthName"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="textPersonName"
        android:text=""
        android:layout_toRightOf="@+id/textViewAuthName"
        android:layout_below="@id/editTextItemInputNumber"
        />

    <TextView
        android:id="@+id/textViewPlace"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="保管場所"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewAuthName"
        />

    <EditText
        android:id="@+id/editTextPlace"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewPlace"
        android:layout_below="@id/editTextAuthName"
        />


    <TextView
        android:id="@+id/textViewItemCode"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="部品\nコード"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewPlace"
        />


    <EditText
        android:id="@+id/editTextItemCode"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemCode"
        android:layout_below="@id/editTextPlace"/>

    <TextView
        android:id="@+id/textViewItemName"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="部品名"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemCode"
        />

    <EditText
        android:id="@+id/editTextItemName"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemName"
        android:layout_below="@id/editTextItemCode"/>

    <TextView
        android:id="@+id/textViewItemSpec"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="型式"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemName"
        />

    <EditText
        android:id="@+id/editTextItemSpec"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemSpec"
