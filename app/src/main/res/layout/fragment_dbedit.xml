        android:text="備考"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemSaler"
        />

    <EditText
        android:id="@+id/editTextItemNote"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemSaler"
        android:layout_below="@id/editTextItemSaler"/>

