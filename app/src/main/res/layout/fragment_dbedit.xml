        />

    <EditText
        android:id="@+id/editTextItemUnitName"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemMaker"
        android:layout_below="@id/editTextItemMaker"/>

    <TextView
        android:id="@+id/textViewItemPriceNew"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="新品価格"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemUnitName"
        />

    <EditText
        android:id="@+id/editTextItemPriceNew"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemUnitName"
        android:layout_below="@id/editTextItemUnitName"/>

    <TextView
        android:id="@+id/textViewItemPriceSec"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="中古価格"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemPriceNew"
        />

    <EditText
        android:id="@+id/editTextItemPriceSec"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
        android:layout_toRightOf="@+id/textViewItemPriceNew"
        android:layout_below="@id/editTextItemPriceNew"/>

    <TextView
        android:id="@+id/textViewItemQtyNew"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="新品個数"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:layout_below="@+id/textViewItemPriceSec"
        />

    <EditText
        android:id="@+id/editTextItemQtyNew"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:background="@drawable/simple_black_flame"
        android:inputType="text"
        android:text=""
